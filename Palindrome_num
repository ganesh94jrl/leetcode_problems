class Solution {
public:
    bool isPalindrome(int x) {
        int n=x;
        if(x<0){
            return false;
        }
        long int num=0;
        while(n){
            num=num*10+n%10;
            n/=10;
        }
        if(num==x){
            return true;
        }
        return false; 
    }
};
